def do(thing):
    return str(thing) + str(1)
print do(5)
51
   
def do(one, two=5):
    return one+two
print do(5)
10

def do(a,b):
    a=5
    b=5
    return a*b
inp = 8
do(inp,5)
print inp

def try_to_change_list_contents(the_list):
    the_list.append('four')
outer_list = ['one', 'two', 'three']
try_to_change_list_contents(outer_list)
print outer_list

def do(a, f):
    return a*f(a)
def inp(a):
    return a*2
print do(6,inp)

def factorial(a):
    n=1
    while a>0:
        n=n*a

    a=a-1
    return n


def fibonacci(y):
    fi_li=[]
    for i in range(1,y+1):
        fi_li.append(fibon(i))
    return fi_li


print fibonacci(10)
def fibon(x):
    if x==0:
        return 0
    elif x==1:
        return 1
    else:
        return fibon(x-1)+fibon(x-2)

















